/* return the largest item in an array, correct */
int max(int arr[], int size){
    int j;
    int result;
    j = 0;
    result = arr[j];
    while(j<=size){
      if(arr[j] > result){
        result = arr[j];
	    j = j+1;
      }
    }
  return result;
}

