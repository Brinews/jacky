<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
 http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>header.jsp</welcome-file>
  </welcome-file-list>
 
  <servlet>
       <description>This is the description of my J2EE component</description>
       <display-name>This is the display name of my J2EE component</display-name>
		 <servlet-name>CosAdd</servlet-name>
		 <servlet-class>beans.CosAdd</servlet-class>
   </servlet>

   <servlet>
		 <servlet-name>EmpUpdate</servlet-name>
		 <servlet-class>beans.EmpUpdate</servlet-class>
   </servlet>

   <servlet-mapping>
       <servlet-name>EmpUpdate</servlet-name>
       <url-pattern>/empUpdate</url-pattern>
     </servlet-mapping>

     <servlet-mapping>
       <servlet-name>CosAdd</servlet-name>
       <url-pattern>/cosAdd</url-pattern>
     </servlet-mapping>
</web-app>

